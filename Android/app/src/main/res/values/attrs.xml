<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActivityView">
        <attr name="icon" format="reference" />
        <attr name="title" format="string" />
        <attr name="start_time" format="string" />
        <attr name="end_time" format="string" />
    </declare-styleable>
    <declare-styleable name="MapOptions">
        <attr name="enabled_menu_icon" format="reference"/>
        <attr name="enabled_menu_title" format="reference"/>
        <attr name="disabled_menu_icon" format="reference"/>
        <attr name="disabled_menu_title" format="reference"/>
        <attr name="menu_enable_by_default" format="boolean"/>
    </declare-styleable>
</resources>